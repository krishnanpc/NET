<?xml version="1.0" encoding="UTF-8"?>
<demo dataFolder=".\\Data">
  <case name="Classification task Libras Movement:  Single spiking reservoir, Feed Forward multilayer network classifier">
    <!-- Data converted from https://archive.ics.uci.edu/ml/datasets/Libras+Movement -->
    <samples fileName="LibrasMovement.csv" singleNormalizer="false" normalizerReserve="0.1"/>
    <stateMachineCfg taskType="Classification" randomizerSeek="0">
      <inputFields routeToReadout="false">
        <field name="coord_abcissa"/>
        <field name="coord_ordinate"/>
      </inputFields>
      <reservoirCfgContainer>
        <reservoirCfg name="Single spiking pool" inputCoding="Analog" inputDuration="1" spectralRadius="0.999">
          <pools>
            <pool name="SimpleIF pool" dimX="6" dimY="6" dimZ="6" readoutNeuronsDensity="1">
              <neuronGroups>
                <neuronGroup name="Exc-SimpleIF" role="Excitatory" relShare="4" augmentedStates="true">
                  <activationSimpleIF stimuliCoeff="1" refractoryPeriods="0">
                    <resistance min="15" max="15"/>
                    <decayRate min="0.05" max="0.05"/>
                    <resetV min="5" max="5"/>
                    <firingThresholdV min="20" max="20"/>
                  </activationSimpleIF>
                  <bias min="0.2" max="0.3" randomSign="false" distribution="Uniform"/>
                </neuronGroup>
                <neuronGroup name="Inh-SimpleIF" role="Inhibitory" relShare="1" augmentedStates="true">
                  <activationSimpleIF stimuliCoeff="0.5" refractoryPeriods="1">
                    <resistance min="15" max="15"/>
                    <decayRate min="0.05" max="0.05"/>
                    <resetV min="5" max="5"/>
                    <firingThresholdV min="20" max="20"/>
                  </activationSimpleIF>
                  <bias min="0" max="0" randomSign="false" distribution="Uniform"/>
                </neuronGroup>
              </neuronGroups>
              <interconnection density="0.1" avgDistance="NA" allowSelfConnection="false" constantNumOfConnections="false">
                <weight min="0" max="1" randomSign="false" distribution="Uniform"/>
              </interconnection>
              <noiseModulator min="-0.8" max="0.8" randomSign="false" distribution="Uniform"/>
            </pool>
          </pools>
        </reservoirCfg>
      </reservoirCfgContainer>
      <reservoirInstanceContainer>
        <reservoirInstance name="Main" cfg="Single spiking pool" augmentedStates="false">
          <inputFieldAssignments>
            <inputFieldAssignment inputFieldName="coord_abcissa" poolName="SimpleIF pool" density="1">
              <weight min="0" max="0.3" randomSign="false" distribution="Uniform"/>
            </inputFieldAssignment>
            <inputFieldAssignment inputFieldName="coord_ordinate" poolName="SimpleIF pool" density="1">
              <weight min="0" max="0.3" randomSign="false" distribution="Uniform"/>
            </inputFieldAssignment>
          </inputFieldAssignments>
        </reservoirInstance>
      </reservoirInstanceContainer>
      <readoutLayer testDataRatio="0.0825" folds="Auto">
        <readoutUnit attempts="10" attemptEpochs="1000">
          <ff regressionMethod="Resilient">
            <activationIdentity/>
            <hiddenLayers>
              <layer neurons="8">
                <activationLeakyReLU>
                  <negSlope min="0.05" max="0.05"/>
                </activationLeakyReLU>
              </layer>
              <layer neurons="8">
                <activationLeakyReLU>
                  <negSlope min="0.05" max="0.05"/>
                </activationLeakyReLU>
              </layer>
            </hiddenLayers>
          </ff>
        </readoutUnit>
        <outputFields>
          <field name="curved swing"/>
          <field name="horizontal swing"/>
          <field name="vertical swing"/>
          <field name="anti-clockwise arc"/>
          <field name="clockwise arc"/>
          <field name="circle"/>
          <field name="horizontal straight-line"/>
          <field name="vertical straight-line"/>
          <field name="horizontal zigzag"/>
          <field name="vertical zigzag"/>
          <field name="horizontal wavy"/>
          <field name="vertical wavy"/>
          <field name="face-up curve"/>
          <field name="face-down curve"/>
          <field name="tremble"/>
        </outputFields>
      </readoutLayer>
    </stateMachineCfg>
  </case>


  <case name="Prediction task High and Low prices of TTOO (T2 Biosystems, Inc.): Directly mixed reservoir (Experimental), Linear regression">
    <!-- Data from https://finance.yahoo.com/quote/TTOO/history?p=TTOO -->
    <samples fileName="TTOO.csv" bootSamples="150" singleNormalizer="true" normalizerReserve="0.1"/>
    <stateMachineCfg taskType="Prediction" randomizerSeek="0">
      <inputFields routeToReadout="true">
        <field name="High"/>
        <field name="Low"/>
        <field name="Adj Close"/>
      </inputFields>
      <reservoirCfgContainer>
        <reservoirCfg name="Directly mixed" spectralRadius="0.9">
          <pools>
            <pool name="Mixed pool" dimX="150" dimY="1" dimZ="1" readoutNeuronsDensity="1">
              <neuronGroups>
                <neuronGroup name="Exc-TanH" role="Excitatory" relShare="10" augmentedStates="true">
                  <activationTanH />
                  <bias min="0" max="0" randomSign="false" distribution="Uniform"/>
                </neuronGroup>
                <neuronGroup name="Inh-TanH" role="Inhibitory" relShare="5" augmentedStates="true">
                  <activationTanH />
                  <bias min="0" max="0" randomSign="false" distribution="Uniform"/>
                </neuronGroup>
                <neuronGroup name="Exc-AutoIzhikevichIF" role="Excitatory" relShare="2" augmentedStates="true">
                  <activationAutoIzhikevichIF stimuliCoeff="70" role="Excitatory" refractoryPeriods="0" solverMethod="RK4" solverCompSteps="2"/>
                  <bias min="0" max="0" randomSign="false" distribution="Uniform"/>
                </neuronGroup>
                <neuronGroup name="Inh-AutoIzhikevichIF" role="Inhibitory" relShare="1" augmentedStates="true">
                  <activationAutoIzhikevichIF stimuliCoeff="20" role="Inhibitory" refractoryPeriods="0" solverMethod="RK4" solverCompSteps="2"/>
                  <bias min="0" max="0" randomSign="false" distribution="Uniform"/>
                </neuronGroup>
              </neuronGroups>
              <interconnection density="0.2" avgDistance="NA" allowSelfConnection="false" constantNumOfConnections="false">
                <weight min="-1" max="1" randomSign="false" distribution="Uniform"/>
              </interconnection>
              <retainmentNeurons density="1">
                <rate min="0.25" max="0.75" randomSign="false" distribution="Uniform"/>
              </retainmentNeurons>
              <noiseModulator min="0" max="0" randomSign="false" distribution="Uniform"/>
            </pool>
          </pools>
        </reservoirCfg>
      </reservoirCfgContainer>
      <reservoirInstanceContainer>
        <reservoirInstance name="Main reservoir" cfg="Directly mixed" augmentedStates="false">
          <inputFieldAssignments>
            <inputFieldAssignment inputFieldName="High" poolName="Mixed pool" density="1">
              <weight min="0" max="0.15" randomSign="true" distribution="Uniform"/>
            </inputFieldAssignment>
            <inputFieldAssignment inputFieldName="Low" poolName="Mixed pool" density="1">
              <weight min="0" max="0.15" randomSign="true" distribution="Uniform"/>
            </inputFieldAssignment>
            <inputFieldAssignment inputFieldName="Adj Close" poolName="Mixed pool" density="1">
              <weight min="0" max="0.15" randomSign="true" distribution="Uniform"/>
            </inputFieldAssignment>
          </inputFieldAssignments>
        </reservoirInstance>
      </reservoirInstanceContainer>
      <readoutLayer testDataRatio="0.1" folds="Auto">
        <readoutUnit attempts="1" attemptEpochs="100">
          <ff regressionMethod="Linear">
            <activationIdentity />
            <linRegrTrainer hiNoiseIntensity="0.2" maxStretch="6" zeroMargin="0.5"/>
          </ff>
        </readoutUnit>
        <outputFields>
          <field name="High"/>
          <field name="Low"/>
        </outputFields>
      </readoutLayer>
    </stateMachineCfg>
  </case>


